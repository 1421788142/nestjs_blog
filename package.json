    "i18n": "^0.15.2",
    "i18n-extract": "^0.6.8",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mockjs": "^1.1.0",
    "multer": "^1.4.5-lts.1",
    "nestjs-i18n": "^10.5.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-binding": "^2.3.16",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
