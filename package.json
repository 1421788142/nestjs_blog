    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/throttler": "^5.0.1",
    "@prisma/client": "^5.6.0",
    "argon2": "^0.31.2",
    "cache-manager": "^5.3.1",
    "class-transformer": "^0.5.1",
